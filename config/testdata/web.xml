<!-- web 的测试文件，需要与其它内容数值相同 -->

<web>
    <root>http://localhost:8082</root>
    <router disableHead="false">
        <pprof>/debug/pprof/</pprof>
        <vars>/debug/vars</vars>
        <static>
            <key name="/admin/{path}">./testdata</key>
        </static>
    </router>
    <readTimeout>3s</readTimeout>
    <timezone>Africa/Addis_Ababa</timezone>
</web>
